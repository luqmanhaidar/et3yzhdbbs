<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ntsky.bbs.domain">
	<class name="Forum" table="ntsky_t_forum" lazy="false">
        <id name="id" column="id">
			<generator class="increment"/>
        </id>
		<property name="name" column="name" not-null="true"/>
		<property name="parentId" column="parent_id" not-null="true"/>
		<property name="parentEnum" column="parent_enum" not-null="true"/>
		<property name="depth" column="depth" not-null="true"/>
		<property name="displayOrder" column="display_order" not-null="true"/>
		<property name="branchId" column="branch_id" not-null="true"/>
		<property name="masters" column="masters" not-null="true"/>
		<property name="description" column="description" not-null="true"/>
		<property name="rules" column="rules" />
		<property name="dateCreated" column="date_created" not-null="true"/>
		<property name="signImage" column="sign_image" />
		<property name="isTop" column="is_top" not-null="true"/>
		<property name="isMasters" column="is_masters" not-null="true"/>
		<property name="isAdmin" column="is_admin" not-null="true"/>
		<!--  <property name="status" column="status" /> -->
		<!--  <property name="totalTopic" column="total_topic" /> -->
		<!--  <property name="totalPost" column="total_post" /> -->
		<!--  <property name="totalTodayPost" column="total_today_post" /> -->
		<!--  <property name="lastPost" column="last_post" /> -->
		
		<!-- <set name="forums" table="ntsky_t_forum" order-by="display_order asc" lazy="false">
            <key column="parent_id"/>
            <one-to-many class="Forum"/>
        </set> -->
		
	</class>
</hibernate-mapping>
