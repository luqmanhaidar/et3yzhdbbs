<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ntsky.bbs.domain">
	<class name="FavoriteTopic" table="ntsky_t_user_favorite_topic" lazy="false">
        <id name="id" column="id">
			<generator class="increment"/>
        </id>
		<property name="userId" column="user_id" not-null="true"/>
		<property name="topicId" column="topic_id" not-null="true"/>

	</class>
</hibernate-mapping>
