<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ntsky.bbs.domain">
	<class name="Announcement" table="ntsky_t_announcement" lazy="false">
        <id name="id" column="id" type="java.lang.Long">
			<generator class="increment" />
        </id>
		<property name="title" column="title" not-null="true"/>
		<property name="content" column="content" not-null="false"/>
		<property name="author" column="author" not-null="true"/>
		<property name="forumId" column="forum_id" not-null="true"/>
		<property name="dateCreated" column="date_created" not-null="true"/>
		<property name="link" column="link" not-null="false"/>
  		<!-- <many-to-one name="forum" 
               insert="false" 
               update="false" 
               column="forum_id" 
               class="Forum"/>  -->             
	</class>
</hibernate-mapping>
