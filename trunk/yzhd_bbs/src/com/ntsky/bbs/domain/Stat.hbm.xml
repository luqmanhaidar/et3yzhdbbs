<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ntsky.bbs.domain">
	<class name="Stat" table="ntsky_t_stat" lazy="true">
        <id name="id" column="id">
			<generator class="increment"/>
        </id>
		<property name="username" not-null="true" column="username"/>
		<property name="place" not-null="true" column="place"/>
		<property name="referer" not-null="true" column="referer"/>
		<property name="agent" not-null="true" column="agent"/>
		<property name="language" not-null="true" column="language"/>
		<property name="viewTime" column="view_time" not-null="true"/>
	</class>
</hibernate-mapping>