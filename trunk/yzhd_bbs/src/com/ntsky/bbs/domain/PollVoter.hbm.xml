<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ntsky.bbs.domain">
	<class name="PollVoter" table="ntsky_t_poll_voter" lazy="false">
	
	    <id name="id" column="id">
			<generator class="increment"/>
        </id>
	
		<property name="pollId" column="poll_id" not-null="true"/>
		<property name="pollResultId" column="poll_result_id" not-null="true"/>
		<property name="pollUser" column="poll_user" not-null="true"/>
		<property name="pollIp" column="poll_ip" not-null="true"/>
	
	</class>
</hibernate-mapping>
