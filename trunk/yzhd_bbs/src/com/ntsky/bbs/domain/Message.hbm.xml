<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ntsky.bbs.domain">
	<class name="Message" table="ntsky_t_message" lazy="true">
        <id name="id" column="id">
			<generator class="increment"/>
        </id>
		<property name="sender" column="sender" not-null="true"/>
		<property name="receiver" column="receiver" not-null="true"/>
		<property name="title" column="title" not-null="true"/>
		<property name="content" column="content" />
		<property name="sendTime" column="send_time" />
		<property name="flag" column="flag" not-null="true"/>
		<property name="isRead" column="is_read" />
		<property name="isDelSender" column="is_del_sender" />
		<property name="isDelReceiver" column="is_del_receiver" />
		<property name="status" column="status" />
	</class>
</hibernate-mapping>
