<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ntsky.bbs.domain">
	<class name="Category" table="ntsky_t_category">
        <id name="id" column="id">
			<generator class="increment"/>
        </id>
		<property name="forumId" column="forum_id" not-null="true"/>
		<property name="name" column="name" not-null="true"/>
		<property name="displayOrder" column="display_order" not-null="true"/>
	</class>
</hibernate-mapping>
