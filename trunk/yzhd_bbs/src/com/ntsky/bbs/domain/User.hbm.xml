<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ntsky.bbs.domain">
	<class name="User" table="ntsky_t_user" lazy="true">
        <id name="id" column="id" unsaved-value="null" type="java.lang.Long">
			<generator class="increment"/>
        </id>
		<property name="username" column="username" not-null="true" unique="true"/>
		<property name="password" column="password" not-null="true"/>
		<property name="sex" column="sex" not-null="true"/>
		<property name="email" column="email" not-null="true"/>
		<property name="question" column="question" not-null="true"/>
		<property name="answer" column="answer" not-null="true"/>
		<property name="roles" column="roles" not-null="true"/>
		<property name="registerTime" column="register_time" not-null="true"/>
		<property name="loginTimes" column="login_times" not-null="true"/>
		<property name="lastLoginTime" column="last_login_time" not-null="true"/>
		<property name="lastLoginIp" column="last_login_ip" not-null="true"/>
		<property name="isLock" column="is_lock" not-null="true"/>
		<property name="isStar" column="is_star" not-null="true"/>
		<property name="birthday" column="birthday" />
		<property name="website" column="website" />
		<property name="imQq" column="im_qq" />
		<property name="imMsn" column="im_msn" />
		<property name="imIcq" column="im_icq" />
		<property name="imYahoo" column="im_yahoo" />
		<property name="gmail" column="gmail" />
		<property name="address" column="address" />
		<property name="face" column="face" />
		<property name="signature" column="signature" />
		<property name="intro" column="intro" />
		<property name="money" column="money" />
		<property name="integral" column="integral" />
		<property name="totalTopic" column="total_topic" />
		<property name="totalPost" column="total_post" />
		<property name="onlineTime" column="online_time" />
		<property name="alias" column="alias" />
	</class>
</hibernate-mapping>
