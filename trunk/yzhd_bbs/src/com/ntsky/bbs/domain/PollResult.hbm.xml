<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ntsky.bbs.domain">
	<class name="PollResult" table="ntsky_t_poll_result" lazy="false">
        <id name="id" column="id">
			<generator class="increment"/>
        </id>
		<property name="pollId" column="poll_id" not-null="true"/>
		<property name="optionId" column="option_id" not-null="true"/>
		<property name="optionText" column="option_text" not-null="true"/>
		<property name="votes" column="votes" not-null="true"/>
	
		<many-to-one name="poll"
	  	      insert="false" 
              update="false" 
              column="poll_id" 
              class="Poll"/>
	</class>
</hibernate-mapping>
