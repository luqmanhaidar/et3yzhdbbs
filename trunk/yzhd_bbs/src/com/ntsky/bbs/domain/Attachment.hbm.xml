<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ntsky.bbs.domain">

    <class name="Attachment" table="ntsky_t_attachment" lazy="false"> 
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
 
        <!--  <property name="topicId" column="topic_id" /> -->
        <property name="folder" column="folder" not-null="true"/>
        <property name="originFilename" column="origin_filename" not-null="true"/>
 		<property name="filename" column="filename" not-null="true"/>
		<property name="filesize" column="filesize" not-null="true"/>
		<property name="filetype" column="filetype"/>
		<property name="comments" column="comments"/>
		<property name="downloadTimes" column="download_times"/>
		<property name="dateCreated" column="date_created" not-null="true"/>
		<property name="userId" column="user_id" not-null="true"/>
    </class>
    
</hibernate-mapping>
