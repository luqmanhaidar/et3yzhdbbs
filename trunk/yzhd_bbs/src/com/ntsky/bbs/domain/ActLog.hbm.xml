<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ntsky.bbs.domain">
	<class name="ActLog" table="ntsky_t_act_log" lazy="true">
        <id name="id" column="act_id" unsaved-value="null">
			<generator class="increment"/>
        </id>
		<property name="name" not-null="true" column="name"/>
		<property name="username" not-null="true" column="username"/>
		<property name="ip" not-null="true" column="ip"/>
		<property name="time" not-null="true" column="time"/>
	</class>
</hibernate-mapping>